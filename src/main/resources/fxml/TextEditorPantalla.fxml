<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="dario.java.std.texteditor.TextEditorPantallaController">
   <children>
      <AnchorPane layoutY="2.0">
         <children>
            <MenuBar prefHeight="25.0" prefWidth="600.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem mnemonicParsing="false" text="New" />
                    <MenuItem fx:id="menuItemOpen" mnemonicParsing="false" onAction="#openFileAction" text="Open" />
                        <MenuItem fx:id="menuItemSave" mnemonicParsing="false" onAction="#saveFileAction" text="Save" />
                        <MenuItem mnemonicParsing="false" text="Save as ..." />
                        <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </AnchorPane>
      <AnchorPane layoutY="27.0">
         <children>
            <TextArea fx:id="texto" prefHeight="371.0" prefWidth="600.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
